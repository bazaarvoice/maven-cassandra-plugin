    <xs:simpleType name="IsbnType" meta:includeIn="product-feed syndication-feed standard-client-feed reviews-display answers-submit reviews-submit stories-submit profiles-submit">
        <xs:restriction base="xs:string">
            <xs:pattern value="[0-9]{8,12}[0-9X]"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- Regular expression that defines a string representation of a locale: Two characters (a-z, lowercase only), and then
