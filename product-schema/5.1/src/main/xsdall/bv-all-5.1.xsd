    <xs:simpleType name="LocaleType" meta:excludeFrom="brand-answers-product-report data-api-input-fields people-feed client-data-standards-report">
        <xs:restriction base="xs:string">
            <xs:pattern value="[a-z]{2}(_[A-Z]{2}(_[0-9a-zA-Z_]+)?)?"/>
        </xs:restriction>
    </xs:simpleType>

