        <xs:restriction base="xs:string">
            <xs:pattern value="[0-9]{8}"/>
            <xs:pattern value="[0-9]{13}"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- Regular expression that limits externalIDs to not contain slashes (/). -->
    <xs:simpleType name="ExternalIdType" meta:excludeFrom="data-api-input-fields">
        <xs:restriction base="xs:string">
            <xs:pattern value="[^/]+"/>
        </xs:restriction>
    </xs:simpleType>

