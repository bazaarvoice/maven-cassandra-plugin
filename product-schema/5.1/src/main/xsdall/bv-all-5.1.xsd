<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:meta="http://www.bazaarvoice.com/xs/meta/1.0"
           xmlns="http://www.bazaarvoice.com/xs/InternalAllXmlBeans/5.1"
           targetNamespace="http://www.bazaarvoice.com/xs/InternalAllXmlBeans/5.1"
           elementFormDefault="qualified"
        >

    <!--
     note: Any <xs:sequence> with a meta:changeToAll="true" attribute can be
     changed to <xs:all> in the schema.

     We always use <xs:sequence> in this file so that the generated XmlBeans
     objects emit elements in the order specified in this schema.  But the
     individual generated schemas should use <xs:all> to set expectations that
     we and our customers should not (and cannot) depend on the order of the
     elements in a complexType.

     By using xs:all instead of xs:sequence, we make it easier to change the
     schema to insert new child elements in the middle of a complex type, while
     migration scripts can add the new elements to the end of the type without
     violating element order constraints.
    -->

    <!-- ** Complex Types ****************************************************************************************** -->

    <!-- AdditionalFieldsType is a collection of AdditionalFieldType elements and is used by the content types (QuestionType, AnswerType, ReviewType, and StoryType).
            It represents an additional free text field that is attached to the content (and may or may not be displayed in the submission form). -->
    <xs:complexType name="AdditionalFieldsType" meta:includeIn="answers-display answers-submit reviews-display reviews-submit stories-display stories-submit profiles-display profiles-submit internal-tools customer-email-alerts syndication-feed standard-client-feed brand-answers-display data-api-query people-feed">
        <xs:sequence>
            <xs:element name="AdditionalField" type="AdditionalFieldType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <!-- An AdditionalFieldType is a value supplied (or to be supplied) by the user as a free-form text field on the content.-->
    <xs:complexType name="AdditionalFieldType" meta:includeIn="answers-display answers-submit reviews-display reviews-submit stories-display stories-submit profiles-display profiles-submit internal-tools customer-email-alerts syndication-feed standard-client-feed brand-answers-display data-api-query people-feed">
        <xs:sequence meta:changeToAll="true">
            <xs:element name="DisplayLabel" type="xs:string" minOccurs="0" meta:includeIn="answers-display answers-submit reviews-display reviews-submit stories-display stories-submit profiles-display profiles-submit internal-tools customer-email-alerts standard-client-feed"/>
            <xs:element name="Label" type="xs:string" minOccurs="0" meta:includeIn="data-api-query"/>
            <xs:element name="SubmissionLabel" type="xs:string" minOccurs="0" meta:includeIn="answers-submit reviews-submit stories-submit profiles-submit internal-tools"/>
            <xs:element name="SubmissionHelpText" type="xs:string" minOccurs="0" meta:includeIn="answers-submit reviews-submit stories-submit profiles-submit internal-tools"/>
            <xs:element name="Value" type="xs:string"/>
            <xs:element name="ValueMarkup" type="MarkupType" minOccurs="0" meta:includeIn="answers-display"/>
        </xs:sequence>
        <xs:attribute name="id" type="ExternalIdType" use="required"/>
        <xs:attribute name="inappropriate" type="xs:boolean" use="optional" meta:includeIn="profiles-submit"/>
    </xs:complexType>

    <!-- A collection of additional questions (on a product/category) related to the review or answer a user just submitted. This is used to prompt users to answer a
            question about a product or category they know something about since they just supplied some content on that subject. -->
    <xs:complexType name="AdditionalQuestionsType" meta:includeIn="answers-submit reviews-submit customer-email-alerts">
        <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element name="Product" type="ProductType" meta:includeIn="answers-submit reviews-submit customer-email-alerts"/>
            <xs:element name="Category" type="CategoryType" meta:includeIn="answers-submit reviews-submit customer-email-alerts"/>
            <xs:element name="Question" type="QuestionType" meta:includeIn="customer-email-alerts"/>
        </xs:choice>
        <xs:attribute name="questionsPerPage" type="PositiveInt" meta:includeIn="answers-submit reviews-submit"/>
    </xs:complexType>

    <xs:complexType name="AlternativeLocaleLinkType" meta:includeIn="answers-display reviews-display stories-display">
        <xs:sequence meta:changeToAll="true">
            <xs:element name="Link" type="xs:anyURI" minOccurs="0"/>
            <xs:element name="Label" type="xs:string" minOccurs="0"/>
            <xs:element name="Locale" type="xs:string" minOccurs="0"/>
            <xs:element name="GroupId" type="xs:string" minOccurs="0"/>
            <xs:element name="Selected" type="xs:boolean" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="AlternativeLocaleLinksType" meta:includeIn="answers-display reviews-display stories-display">
        <xs:sequence>
            <xs:element name="AlternativeLocaleLink" type="AlternativeLocaleLinkType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <!-- Collection of links used on pages which have no particular subject, e.g. the root level of the microsite or the standalone search results page. -->
    <xs:complexType name="AllSubjectsLinksType" meta:includeIn="answers-display answers-submit reviews-display reviews-submit stories-submit profiles-submit">
        <xs:sequence meta:changeToAll="true">
            <xs:element name="AllQuestionsLink" type="xs:anyURI" minOccurs="0" meta:includeIn="answers-display"/>
            <xs:element name="AllReviewsLink" type="xs:anyURI" minOccurs="0" meta:includeIn="reviews-display"/>
            <xs:element name="AllReviewsFullSiteLink" type="xs:anyURI" minOccurs="0" meta:includeIn="reviews-display"/>
            <xs:element name="AllReviewsMobileSiteLink" type="xs:anyURI" minOccurs="0" meta:includeIn="reviews-display"/>
            <xs:element name="AllStoriesEmbeddedLink" type="xs:anyURI" minOccurs="0" meta:includeIn="stories-display"/>
            <xs:element name="CategoryNavigationLinks" type="CategoryNavigationLinksType" minOccurs="0" meta:includeIn="answers-display answers-submit profiles-submit reviews-display reviews-submit stories-submit"/>
            <xs:element name="MostRecentQuestionsLinks" type="RecentQuestionLinksType" minOccurs="0" meta:includeIn="answers-display"/>
            <xs:element name="MostRecentQuestionsWithAnswerLinks" type="RecentQuestionLinksType" minOccurs="0" meta:includeIn="answers-display"/>
            <xs:element name="MostPopularProductsLinks" type="RankedProductLinksType" minOccurs="0" meta:includeIn="reviews-display"/>
            <xs:element name="TopRatedProductsLinks" type="RankedProductLinksType" minOccurs="0" meta:includeIn="reviews-display"/>
            <xs:element name="CategoryShoppingLinks" type="CategoryNavigationLinksType" minOccurs="0" meta:includeIn="answers-display reviews-display"/>
            <xs:element name="SearchQuestionsSubmitLink" type="SearchSubmitLinkType" minOccurs="0" meta:includeIn="answers-display"/>
            <xs:element name="SearchProductsSubmitLink" type="SearchSubmitLinkType" minOccurs="0" meta:includeIn="answers-submit profiles-submit reviews-submit stories-submit"/>
            <xs:element name="InstantSearchProductsSubmitLink" type="SearchSubmitLinkType" minOccurs="0" meta:includeIn="answers-submit profiles-submit reviews-submit stories-submit"/>
            <xs:element name="BrowseAllProductsLink" type="SearchSubmitLinkType" minOccurs="0" meta:includeIn="answers-submit profiles-submit reviews-submit stories-submit"/>
            <xs:element name="AlternativeLocalesLinks" type="AlternativeLocaleLinksType" minOccurs="0" meta:includeIn="answers-display reviews-display stories-display"/>
        </xs:sequence>
    </xs:complexType>

    <!-- Top level element that represents all subjects (products and categories).-->
    <xs:complexType name="AllSubjectsType" meta:includeIn="answers-display answers-submit reviews-display reviews-submit stories-submit profiles-submit">
        <xs:sequence meta:changeToAll="true">
            <xs:element name="Links" type="AllSubjectsLinksType" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <!-- Collection of Links relevant to rendering an AnswerType element. -->
    <xs:complexType name="AnswerLinksType" meta:includeIn="answers-display answers-submit profiles-display customer-email-alerts">
        <xs:sequence meta:changeToAll="true">
            <xs:element name="AuthenticationConfirmationLink" type="xs:anyURI" minOccurs="0" meta:includeIn="answers-submit customer-email-alerts"/>
            <xs:element name="PositiveFeedbackLink" type="xs:anyURI" minOccurs="0" meta:includeIn="answers-display"/>
            <xs:element name="NegativeFeedbackLink" type="xs:anyURI" minOccurs="0" meta:includeIn="answers-display"/>
            <xs:element name="UndoHelpfulnessFeedbackLink" type="xs:anyURI" minOccurs="0" meta:includeIn="answers-display"/>
            <xs:element name="BestAnswerFeedbackLink" type="xs:anyURI" minOccurs="0" meta:includeIn="answers-display"/>
            <xs:element name="InappropriateFeedbackLink" type="xs:anyURI" minOccurs="0" meta:includeIn="answers-display"/>
            <xs:element name="UserProfileLink" type="xs:anyURI" minOccurs="0" meta:excludeFrom="profiles-display customer-email-alerts"/>
            <xs:element name="PhotoUploadLink" type="xs:anyURI" minOccurs="0" meta:includeIn="answers-submit"/>
            <xs:element name="VideoUploadLink" type="xs:anyURI" minOccurs="0" meta:includeIn="answers-submit"/>
            <xs:element name="SearchProductsSubmitLink" type="xs:anyURI" minOccurs="0" meta:includeIn="answers-submit"/>
            <xs:element name="ReturnLink" type="xs:anyURI" minOccurs="0" meta:includeIn="answers-submit"/>
            <xs:element name="ContentRedirectLink" type="xs:anyURI" minOccurs="0" meta:includeIn="answers-display profiles-display answers-submit customer-email-alerts"/>
        </xs:sequence>
    </xs:complexType>

    <!-- Element that describes an individual instance of Answer content. This includes the current status in moderation, the fields supplied
            during submission, and internal only data like IpAddress, the session parameters, etc.-->
    <xs:complexType name="AnswerType" meta:includeIn="answers-display answers-submit profiles-display syndication-feed standard-client-feed brand-answers-display brand-answers-all-qa-report customer-email-alerts data-api-query data-api-submit workbench-recurring-alerts">
        <xs:sequence meta:changeToAll="true">
            <xs:element name="ModerationStatus" type="ModerationStatusType" minOccurs="0" meta:includeIn="syndication-feed standard-client-feed brand-answers-display brand-answers-all-qa-report data-api-query"/>
            <xs:element name="WorkflowState" type="WorkflowStateType" minOccurs="0" meta:includeIn="brand-answers-display"/>
            <xs:element name="LastModificationTime" type="xs:dateTime" minOccurs="0" meta:includeIn="syndication-feed standard-client-feed data-api-query"/>
            <xs:element name="Source" type="ContentSourceType" minOccurs="0" meta:includeIn="answers-display answers-submit"/>
            <xs:element name="EnsureVisible" type="xs:boolean" minOccurs="0" meta:includeIn="answers-display"/>
            <xs:element name="SubmissionTime" type="xs:dateTime" minOccurs="0"/>
            <xs:element name="ElapsedTime" type="xs:string" minOccurs="0" meta:includeIn="answers-display answers-submit profiles-display brand-answers-display"/>
            <xs:element name="NumFeedbacks" type="NonNegativeInt" minOccurs="0" meta:excludeFrom="customer-email-alerts data-api-query data-api-submit"/>
            <xs:element name="NumPositiveFeedbacks" type="NonNegativeInt" minOccurs="0" meta:excludeFrom="customer-email-alerts data-api-query data-api-submit"/>
            <xs:element name="NumNegativeFeedbacks" type="NonNegativeInt" minOccurs="0" meta:excludeFrom="customer-email-alerts data-api-query data-api-submit"/>
            <xs:element name="TotalFeedbackCount" type="NonNegativeInt" minOccurs="0" meta:includeIn="data-api-query"/>
            <xs:element name="TotalPositiveFeedbackCount" type="NonNegativeInt" minOccurs="0" meta:includeIn="data-api-query"/>
            <xs:element name="TotalNegativeFeedbackCount" type="NonNegativeInt" minOccurs="0" meta:includeIn="data-api-query"/>
            <xs:element name="NumBestAnswerFeedbacks" type="NonNegativeInt" minOccurs="0" meta:includeIn="brand-answers-all-qa-report"/>
            <xs:element name="NumInappropriateFeedbacks" type="NonNegativeInt" minOccurs="0" meta:includeIn="standard-client-feed"/>
            <xs:element name="Badges" type="BadgesType" minOccurs="0" meta:excludeFrom="brand-answers-all-qa-report data-api-submit workbench-recurring-alerts" meta:jsonMapKey="id"/>
            <xs:element name="User" type="UserProfileType" minOccurs="0" meta:excludeFrom="standard-client-feed syndication-feed brand-answers-all-qa-report data-api-query data-api-submit workbench-recurring-alerts"/>
            <xs:element name="UserProfileReference" type="UserProfileReferenceType" minOccurs="0" meta:includeIn="standard-client-feed syndication-feed"/>
            <xs:element name="UserNickname" type="xs:string" minOccurs="0" meta:excludeFrom="data-api-submit"/>
            <xs:element name="UserLocation" type="xs:string" minOccurs="0" meta:excludeFrom="brand-answers-all-qa-report data-api-submit"/>
            <xs:element name="UserLocationGeocode" type="GeocodeType" minOccurs="0" meta:includeIn="answers-submit standard-client-feed"/>
            <xs:element name="ContextDataValues" type="ContextDataValuesType" minOccurs="0" meta:excludeFrom="brand-answers-all-qa-report data-api-submit workbench-recurring-alerts" meta:jsonMapKey="id"/>
            <xs:element name="Photos" type="PhotosType" minOccurs="0" meta:excludeFrom="customer-email-alerts data-api-submit workbench-recurring-alerts"/>
            <xs:element name="Videos" type="VideosType" minOccurs="0" meta:excludeFrom="customer-email-alerts data-api-submit workbench-recurring-alerts"/>
            <xs:element name="ProductReferences" type="ProductReferencesType" minOccurs="0" meta:excludeFrom="customer-email-alerts data-api-submit data-api-query workbench-recurring-alerts"/>
            <xs:element name="ProductRecommendationIds" type="ExternalIdsType" minOccurs="0" meta:includeIn="data-api-query"/>
            <xs:element name="AnswerText" type="xs:string" minOccurs="0"/>
            <xs:element name="AnswerTextMarkup" type="MarkupType" minOccurs="0" meta:includeIn="answers-display answers-submit profiles-display"/>
            <xs:element name="AdditionalFields" type="AdditionalFieldsType" minOccurs="0" meta:excludeFrom="brand-answers-all-qa-report data-api-submit workbench-recurring-alerts" meta:jsonMapKey="id"/>
            <xs:element name="Links" type="AnswerLinksType" minOccurs="0" meta:includeIn="answers-display answers-submit profiles-display customer-email-alerts"/>

            <xs:element name="TypicalHoursToPost" type="NonNegativeInt" minOccurs="0" meta:includeIn="answers-submit brand-answers-display data-api-submit"/>
            <xs:element name="RejectedDisplayName" type="xs:string" minOccurs="0" meta:includeIn="answers-submit"/>
            <xs:element name="DisplayAsAnonymous" type="xs:boolean" minOccurs="0" meta:includeIn="answers-submit"/>
            <xs:element name="ConfirmationEmailPosted" type="xs:boolean" minOccurs="0" meta:includeIn="answers-submit"/>
            <xs:element name="AuthenticationType" type="AuthenticationType" minOccurs="0" meta:includeIn="standard-client-feed"/>
            <xs:element name="UserFacebookID" type="xs:string" minOccurs="0" meta:includeIn="standard-client-feed"/>
            <xs:element name="EncryptedUserFacebookID" type="xs:string" minOccurs="0" meta:includeIn="answers-display profiles-display"/>
            <xs:element name="SocialConnectPhotoUrl" type="xs:anyURI" minOccurs="0" meta:includeIn="answers-display profiles-display"/>
            <xs:element name="UserEmailAddress" type="xs:string" minOccurs="0" meta:includeIn="answers-submit standard-client-feed"/>
            <xs:element name="RememberMeLoggedIn" type="xs:boolean" minOccurs="0" meta:includeIn="answers-submit"/>
            <xs:element name="SendEmailAlertWhenPublished" type="xs:boolean" minOccurs="0" meta:includeIn="answers-submit standard-client-feed data-api-submit"/>
            <xs:element name="AgreedToTermsAndConditions" type="xs:boolean" minOccurs="0" meta:includeIn="answers-submit"/>
            <xs:element name="SubmitForm" type="FormType" minOccurs="0" meta:includeIn="answers-submit"/>
            <xs:element name="MediaUploadForm" type="FormType" minOccurs="0" meta:includeIn="answers-submit"/>
            <xs:element name="InappropriateFeedbackForm" type="FormType" minOccurs="0" meta:includeIn="answers-submit"/>
            <xs:element name="ReasonText" type="xs:string" minOccurs="0" meta:includeIn="answers-submit"/>
            <xs:element name="SocialConnectSites" type="SocialConnectSitesType" minOccurs="0" meta:includeIn="answers-submit"/>
            <xs:element name="SubjectAnswersDeepLinkedUrl" type="xs:anyURI" minOccurs="0" meta:includeIn="syndication-feed standard-client-feed"/>

            <xs:element name="IpAddress" type="xs:string" minOccurs="0" meta:includeIn="answers-submit standard-client-feed"/>
            <xs:element name="SessionParams" type="xs:string" minOccurs="0" meta:includeIn="standard-client-feed"/>
            <xs:element name="DisplayLocale" type="LocaleType" minOccurs="0" meta:excludeFrom="data-api-submit data-api-query"/>
            <xs:element name="ContentLocale" type="LocaleType" minOccurs="0" meta:includeIn="data-api-query"/>
            <xs:element name="CampaignId" type="ExternalIdType" minOccurs="0" meta:includeIn="standard-client-feed data-api-query"/>
            <xs:element name="ContentCodes" type="xs:string" minOccurs="0" meta:includeIn="answers-display customer-email-alerts standard-client-feed brand-answers-all-qa-report"/>
            <xs:element name="OriginatingDisplayCode" type="DisplayCodeType" minOccurs="0" meta:includeIn="standard-client-feed answers-submit customer-email-alerts"/>
            <xs:element name="FirstPublishTime" type="xs:dateTime" minOccurs="0" meta:includeIn="standard-client-feed brand-answers-all-qa-report"/>
            <xs:element name="LastPublishTime" type="xs:dateTime" minOccurs="0" meta:includeIn="standard-client-feed answers-submit customer-email-alerts"/>
            <xs:element name="LastModeratedTime" type="xs:dateTime" minOccurs="0" meta:includeIn="data-api-query"/>
            <xs:element name="SubmissionID" type="xs:string" minOccurs="0" meta:includeIn="answers-submit"/>
            <xs:element name="SubmissionId" type="xs:string" minOccurs="0" meta:includeIn="data-api-query data-api-submit"/>
            <xs:element name="QuestionId" type="ExternalIdType" minOccurs="0" meta:includeIn="data-api-query"/>
            <xs:element name="AuthorId" type="ExternalIdType" minOccurs="0" meta:includeIn="data-api-query"/>
            <xs:element name="IsFeatured" type="xs:boolean" minOccurs="0" meta:includeIn="data-api-query"/>
            <xs:element name="ModeratorHighlights" type="ModeratorHighlightsType" minOccurs="0" meta:includeIn="data-api-query" meta:jsonMapKey="id" meta:jsonNullify="false"/>
        </xs:sequence>
        <xs:attribute name="id" type="ExternalIdType"/>
    </xs:complexType>

    <!-- Collection of Answers. -->
    <xs:complexType name="AnswersType" meta:includeIn="answers-display answers-submit profiles-display syndication-feed standard-client-feed brand-answers-display brand-answers-all-qa-report customer-email-alerts data-api-query workbench-recurring-alerts">
        <xs:sequence>
            <xs:element name="Answer" type="AnswerType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <!-- Append the submission form of one application after the container of the current submission flow. -->
    <xs:complexType name="AppendSubmissionType" meta:includeIn="answers-submit reviews-submit stories-submit">
        <xs:sequence meta:changeToAll="true">
            <xs:element name="ApplicationCode" type="xs:string"/>
            <xs:element name="SubmissionUrl" type="xs:anyURI"/>
            <xs:element name="StaticResourceBaseUrl" type="xs:anyURI"/>
        </xs:sequence>
    </xs:complexType>

    <!-- Collection of user profiles for the Data API. -->
    <xs:complexType name="AuthorsType" meta:includeIn="data-api-query">
        <xs:sequence>
            <xs:element name="Author" type="UserProfileType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <!-- Type of Reviewer Authentication. -->
    <xs:simpleType name="AuthenticationType" meta:includeIn="standard-client-feed">
        <xs:restriction base="xs:string">
            <xs:enumeration value="EMAIL"/>
            <xs:enumeration value="FACEBOOK"/>
            <xs:enumeration value="SITE"/>
        </xs:restriction>
    </xs:simpleType>

    <!-- An attribute to filter content on, such as overall rating, a rating dimension, or a context data dimension. -->
    <xs:complexType name="AttributeFilteringDimension" meta:includeIn="reviews-display answers-display stories-display">
        <xs:sequence meta:changeToAll="true">
            <xs:element name="Label" type="xs:string" minOccurs="0"/>
            <xs:element name="Title" type="xs:string" minOccurs="0"/>
            <xs:element name="AttributeFilteringLinks" type="AttributeFilteringLinksInDimensionType"/>
            <xs:element name="InitiallyVisibleOptionsCount" type="NonNegativeInt" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="id" type="ExternalIdType" use="required"/>
        <xs:attribute name="disabled" type="xs:boolean"/>
        <xs:attribute name="expanded" type="xs:boolean"/>
        <xs:attribute name="noeffect" type="xs:boolean"/>
        <xs:attribute name="useMostPopularAsTitle" type="xs:boolean"/>
        <xs:attribute name="useMouseover" type="xs:boolean"/>
        <xs:attribute name="inlineHistogramEnabled" type="xs:boolean"/>
    </xs:complexType>

    <!-- Collection of AttributeFilteringLinkType elements. -->
    <xs:complexType name="AttributeFilteringLinksInDimensionType" meta:includeIn="reviews-display answers-display stories-display">
        <xs:sequence>
            <xs:element name="AttributeFilteringLink" type="AttributeFilteringLinkType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <!-- Collection of AttributeDimension elements that contain AttributeFilteringLinkType elements. -->
    <xs:complexType name="AttributeFilteringLinksType" meta:includeIn="reviews-display answers-display stories-display">
        <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element name="ContextDataDimension" type="AttributeFilteringDimension"/>
            <xs:element name="OverallRating" type="AttributeFilteringDimension" meta:includeIn="reviews-display"/>
            <xs:element name="RatingDimension" type="AttributeFilteringDimension" meta:includeIn="reviews-display"/>
            <xs:element name="BadgeGroup" type="AttributeFilteringDimension"/>
            <xs:element name="ContentSource" type="AttributeFilteringDimension" meta:includeIn="reviews-display"/>
            <xs:element name="ContentSubject" type="AttributeFilteringDimension"/>
            <xs:element name="TagDimension" type="AttributeFilteringDimension"/>
            <xs:element name="ContentLocale" type="AttributeFilteringDimension"/>
        </xs:choice>
    </xs:complexType>

    <!-- AttributeFilteringLinkType embodies the link to add/remove attribute filtering for the attribute with this label, including the URI. -->
    <xs:complexType name="AttributeFilteringLinkType" meta:includeIn="reviews-display answers-display stories-display">
        <xs:sequence meta:changeToAll="true">
            <xs:element name="Label" type="xs:string"/>
            <xs:element name="Link" type="xs:anyURI" minOccurs="0"/>
            <xs:element name="MatchCount" type="NonNegativeInt"/>
        </xs:sequence>
        <xs:attribute name="defaultSelected" type="xs:boolean"/>
        <xs:attribute name="selected" type="xs:boolean"/>
        <xs:attribute name="id" type="ExternalIdType" use="optional"/>
    </xs:complexType>

    <!-- AttributesType is a collection of AttributeType elements attached to a Product or Category. These attributes are client defined in the product data feed and
            include things like ManufacturerPartNumber, ISBN, etc. or other completely custom attributes.-->
    <xs:complexType name="AttributesType" meta:includeIn="answers-display answers-submit customer-email-alerts internal-tools product-feed profiles-display profiles-submit reviews-display reviews-submit standard-client-feed stories-display stories-submit syndication-feed brand-answers-display data-api-query">
        <xs:sequence>
            <xs:element name="Attribute" type="AttributeType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

    <!-- A single AttributeType can hold all of the localized values for that attribute. It consists of an externalID and a sequence of either values or urls that may or may not be localized. -->
    <xs:complexType name="AttributeType" meta:includeIn="answers-display answers-submit customer-email-alerts internal-tools product-feed profiles-display profiles-submit reviews-display reviews-submit standard-client-feed stories-display stories-submit syndication-feed brand-answers-display data-api-query">
        <xs:choice>
            <xs:element name="Value" type="OptionalLocalizedStringType" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element name="UrlValue" type="OptionalLocalizedUrlType" minOccurs="1" maxOccurs="unbounded"/>
        </xs:choice>
        <xs:attribute name="id" type="ExternalIdType" use="required"/>
    </xs:complexType>

    <!-- AvatarType contains information about the image used to represent a user throughout the application -->
    <xs:complexType name="AvatarType" meta:includeIn="profiles-display profiles-submit answers-display answers-submit reviews-display reviews-submit stories-display stories-submit customer-email-alerts standard-client-feed data-api-query">
        <xs:sequence meta:changeToAll="true">
            <xs:element name="Sizes" type="PhotoSizesType" minOccurs="0"/>
        </xs:sequence>
        <!-- ID of the user photo or the predefined avatar -->
        <xs:attribute name="id" type="ExternalIdType" use="required"/>
        <!-- Determines whether the avatar is a user photo (versus a predefined avatar) -->
        <xs:attribute name="photo" type="xs:boolean" use="required"/>
    </xs:complexType>

    <!-- AverageRatingValueType includes the floating point value for the average rating, the URL to the appropriate image for this client,
            and a reference to the relevant rating dimension.-->
    <xs:complexType name="AverageRatingValueType" meta:includeIn="syndication-feed standard-client-feed product-feed reviews-display reviews-submit">
        <xs:sequence meta:changeToAll="true">
            <xs:element name="AverageRating" type="xs:float"/>
            <xs:element name="AverageRatingImageUrl" type="ImageUrlType" minOccurs="0" meta:includeIn="reviews-display reviews-submit"/>
            <xs:element name="AverageRatingImageUrlSprite" type="ImageUrlSpriteType" minOccurs="0" meta:includeIn="reviews-display reviews-submit"/>
            <xs:element name="RatingDimension" type="RatingDimensionType"/>
            <xs:element name="TotalReviewCount" type="xs:int" minOccurs="0" meta:includeIn="reviews-display"/>
            <xs:element name="RatingDistribution" type="RatingDistributionType" minOccurs="0" meta:includeIn="reviews-display" />
        </xs:sequence>
        <!--Deprecated copy of the RatingDimension ID.  Should be removed in next version of the schema-->
