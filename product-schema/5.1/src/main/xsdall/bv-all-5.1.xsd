    <!-- Regular expression that defines a display code: Must consist of one ore more letters (a-z, either case), numbers (0-9), underscores (_) and/or hyphens (-). -->
    <xs:simpleType name="DisplayCodeType" meta:includeIn="answers-display answers-submit reviews-display reviews-submit stories-display stories-submit profiles-display profiles-submit internal-tools customer-email-alerts standard-client-feed syndication-feed client-data-standards-report">
